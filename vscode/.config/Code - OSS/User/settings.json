{
  // "editor.fontSize": 12,
  "editor.fontFamily": "Terminus \\(TTF\\)",
  "editor.fontSize": 15,
  "editor.lineHeight": 13,
  "editor.fontLigatures": true,
  "breadcrumbs.icons": true,
  "breadcrumbs.filePath": "off",
  "window.zoomLevel": 0,
  "editor.renderIndentGuides": true,
  "editor.minimap.enabled": false,
  "editor.folding": true,
  "editor.wordWrap": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.tabCloseButton": "off",
  "editor.lineNumbers": "relative",
  "editor.glyphMargin": true,
  "editor.tabSize": 2,
  "zenMode.hideTabs": false,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "scm.diffDecorations": "gutter",
  "editor.formatOnSave": false,
  "editor.minimap.showSlider": "always",
  "explorer.confirmDelete": false,
  "terminal.integrated.fontFamily": "Terminus \\(TTF\\)",
  // "terminal.integrated.fontSize": 12,
  // "terminal.integrated.fontFamily": "Jetbrains Mono",
  "terminal.integrated.fontSize": 11,
  "debug.console.fontSize": 11,
  "debug.console.fontFamily": "Terminus \\(TTF\\)",
  "zenMode.fullScreen": false,
  "terminal.external.linuxExec": "alacritty",
  "go.useLanguageServer": true,
  "editor.stickyTabStops": true,
  "window.title": "${rootName} - ${dirty}${activeEditorMedium}",
  "zenMode.centerLayout": true,
  // "window.title": "${dirty}${activeEditorMedium}${rootName}",
  "editor.tokenColorCustomizations": {
    "[White Night]": {
      "comments": "#C3E88D",
      "functions": {
        "fontStyle": "bold",
        "foreground": "#808080"
      },
      "keywords": {
        "foreground": "#808080"
      },
      "types": {
        "foreground": "#a7a7a7"
      },
      "strings": "#b3b3b3",
      "variables": {
        "foreground": "#929292"
      },
    },
  },
  "workbench.colorCustomizations": {
    "[White Night]": {
      "editor.foreground": "#ffffff",

      "terminal.background": "#000",
      "terminal.foreground": "#ccc",
      "terminalCursor.background": "#EEFFFF",
      "terminalCursor.foreground": "#EEFFFF",
      "terminal.ansiBlack": "#7F7F7F",
      "terminal.ansiBlue": "#82AAFF",
      "terminal.ansiBrightBlack": "#4A4A4A",
      "terminal.ansiBrightBlue": "#82AAFF",
      "terminal.ansiBrightCyan": "#89DDFF",
      "terminal.ansiBrightGreen": "#C3E88D",
      "terminal.ansiBrightMagenta": "#C792EA",
      "terminal.ansiBrightRed": "#F07178",
      "terminal.ansiBrightWhite": "#FFFFFF",
      "terminal.ansiBrightYellow": "#FFCB6B",
      "terminal.ansiCyan": "#89DDFF",
      "terminal.ansiGreen": "#C3E88D",
      "terminal.ansiMagenta": "#C792EA",
      "terminal.ansiRed": "#F07178",
      "terminal.ansiWhite": "#EEFFFF",
      "terminal.ansiYellow": "#FFCB6B",
      "statusBar.foreground": "#7f8fa6",
      "tab.inactiveForeground": "#a4b0be",
      "editorLineNumber.activeForeground": "#7f8fa6",
      "editorLineNumber.foreground": "#353b48",
      "editor.lineHighlightBackground": "#102020"
    },
  },
  "editor.minimap.renderCharacters": false,
  "explorer.confirmDragAndDrop": false,
  "editor.cursorStyle": "block",
  "workbench.iconTheme": null,
  "workbench.statusBar.visible": true,
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.dataScience.jupyterServerURI": "local",
  "python.formatting.provider": "autopep8",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.linting.banditEnabled": false,
  "timeline.excludeSources": [],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "latex-workshop.view.pdf.viewer": "external",
  "cSpell.enabled": true,
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
  ],
  "workbench.colorTheme": "White Night",
  "workbench.editor.showTabs": false,
  "window.menuBarVisibility": "hidden",
  "workbench.activityBar.visible": false
}